import random
num = random.choice(xrange(1000, 9999))
num1 = []
i = 0
while i < 4:
    j = num%10
    num1.insert(0,j)
    num = num/10
    i += 1print num1
user = int(raw_input("Guess the 4 digit number generated by computer and win prizes \n you have 5 attemps to guess correct number.Enter your choice now:"))
